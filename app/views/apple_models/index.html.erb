<p id="notice"><%= notice %></p>

<h1>Apple Models</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @apple_models.each do |apple_model| %>
      <tr>
        <td><%= link_to 'Show', apple_model %></td>
        <td><%= link_to 'Edit', edit_apple_model_path(apple_model) %></td>
        <td><%= link_to 'Destroy', apple_model, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Apple Model', new_apple_model_path %>
